{
    "manifest_version": 3,
    "name": "Font Changer",
    "version": "1.0.0",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  